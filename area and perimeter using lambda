l=int(input("Enter the length of rectangle:"))
b=int(input("Enter the breadth of rectangle:"))
s=int(input("Enter the side of a square:"))
area=lambda l,b :l*b 
peri=lambda l,b :2*(l+b)
sarea=lambda s :s*s
speri=lambda s :4*s 
print("Area of rectangle is:",area(l,b))
print("Perimeter of rectangle is:",peri(l,b))
print("Area of square is:",sarea(s))
print("Perimeter of square is:",speri(s))
