a=10
b=5
print("Swapping without temporary variable:")
print("a before swapping",a)
print("b before swapping",b)
a=a+b
b=a-b
a=a-b
print("a after swapping",a)
print("b after swapping",b)
a=30
b=25
print("\n\nSwapping with temporary variable:")
print("a before swapping",a)
print("b before swapping",b)
temp=a
a=b 
b=temp
print("a after swapping",a)
print("b after swapping",b)
