class rect:
    def __init__(self,length,breadth):
        self.length=length
        self.breadth=breadth
    def area(self):
        return self.length*self.breadth
    def peri(self):
        return 2*(self.length+self.breadth)
l=int(input("Enter the length:"))
b=int(input("Enter the breadth:"))
obj=rect(l,b)
print("Area=",obj.area())
print("Breadth=",obj.peri())
